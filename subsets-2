class Solution:
    def subsetsWithDup(self, nums: List[int]) -> List[List[int]]:
        subSet, currSet = [],[]
        nums = sorted(nums)
        n = len(nums)
        def helper(i,nums):
            if i>=n:
                subSet.append(currSet.copy())
                return
            currSet.append(nums[i])
            helper(i+1,nums)
            currSet.pop()
            while i+1<n and nums[i]==nums[i+1]:
                i+=1
            helper(i+1,nums)

        helper(0, nums)
        return subSet
            
