class Solution:
    def longestCommonPrefix(self,strs,):
        if(len(strs[0])==0):
            return ""
        strs.sort()    
        m=len(strs)
        first=strs[0]
        last=strs[m-1]
        n=min(len(first),len(last))
        resultString=""
        for i in range (0,n):
            if(first[i]!=last[i]):
                return resultString
            resultString+=first[i]
        return resultString        
